    "class Softmax:\n",
    "    @classmethod\n",
    "    def get_arm(cls, state: State, tau: float=0.05) -> int:\n",
    "        tau = 1000/ (sum(state.counts) + 1)**2\n",
    "        logit = state.values / tau\n",
    "        logit = logit - np.max(logit)\n",
    "        p = np.exp(logit) / sum(np.exp(logit))\n",
    "        arm = np.random.choice(state.n_arms, p=p)\n",
    "        return arm"
