    "class ThompsonSampling:\n",
    "    @classmethod\n",
    "    def get_arm(cls, state: TSState) -> int:\n",
    "        beta = lambda N, a: np.random.beta(a + 1, N - a + 1)\n",
    "        dist = [beta(state.counts[i], state.values[i]) for i in range(state.n_arms)]\n",
    "        arm = dist.index(max(dist))\n",
